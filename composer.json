{
    "name": "ecommit/crud-bundle",
    "description": "EcommitCrudBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "doctrine/common": "*",
        "doctrine/doctrine-bundle": "^1.12.3|^2.0.3",
        "doctrine/orm": "^2.6.3",
        "doctrine/persistence": "^1.3",
        "ecommit/javascript-bundle": "^2.5.1",
        "ecommit/util-bundle": "^2.5.1",
        "symfony/asset": "^4.4|^5.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/doctrine-bridge": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/routing": "^4.4|^5.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "twig/extensions": "^1.0",
        "twig/twig": "^2.12.0|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "phpunit/phpunit": "^8.4",
        "symfony/dom-crawler": "^4.4|^5.0",
        "symfony/panther": "^0.7.1",
        "symfony/webpack-encore-bundle": "^1.7.3"
    },
    "autoload": {
        "psr-4": { "Ecommit\\CrudBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ecommit\\CrudBundle\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
