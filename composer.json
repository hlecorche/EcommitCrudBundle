        "phpunit/phpunit": "^8.4",
        "symfony/dom-crawler": "^4.4|^5.0",
        "symfony/http-client": "^4.4.11|^5.1.3",
        "symfony/panther": "^0.9.0",
        "symfony/webpack-encore-bundle": "^1.7.3",
        "symfony/yaml": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": { "Ecommit\\CrudBundle\\": "src/" }
